<div class="container">
  <h1 class="col-lg-12 page-title">Address Book</h1>
  <div class="row">
    <div class="col-lg-6 col-lg-offset-3">
      <div class="panel">
        <div class="panel-body">
          {{#each addresses}}
            <strong>
              Address {{@index}}
            </strong>
            {{> address }}
          {{/each}}
        </div>
      </div>
    </div>
  </div>
</div>
