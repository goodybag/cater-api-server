{{#each orders}}
<tr class="table-row" data-order-type="{{type}}">
  <td><a href="/orders/{{id}}">#{{id}}</td>
  <td>{{capitalize type}} {{#eq type 'courier'}}(<a href="/admin/delivery-services/{{deliveryService.id}}">{{deliveryService.name}}</a>){{/eq}}</td>
  <td><input type="checkbox" {{#if restaurant.has_contract}}checked="checked"{{/if}} disabled></td>
  <td>{{datepart datetime}} {{timepart datetime}}</td>
  <td><a href="/admin/users/{{user.id}}">{{user.email}}</a></td>
  <td>{{user.organization}}</td>
  <td><a href="/admin/restaurants/{{restaurant.id}}">{{restaurant.name}}</a></td>
  <td>
    {{~#if location~}}
    <a href="/admin/restaurants/{{restaurant.id}}/locations/{{location.id}}">{{location.name}}</a>
    {{~else~}}
    N/A
    {{~/if~}}
  </td>
  <td><a href="/admin/regions">{{restaurant.region.name}}</a></td>
</tr>
{{/each}}
