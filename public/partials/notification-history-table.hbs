<thead>
  <tr>
    <th>Recipients</th>
    <th>Name</th>
    <th>Send Date</th>
    <th>Actions</th>
  </tr>
</thead>
<tbody>
  {{#each items}}
  <tr>
    <td>{{{list this.email.to}}}</td>
    <td>{{this.name}}</td>
    <td>{{formatDateTime this.send_date "MM/DD/YYYY h:mma"}}</td>
    <td class="actions">
      <button
        class="btn btn-small btn-default btn-highlight"
        data-cid="{{this.cid}}"
      >Highlight in Available</button>
    </td>
  </tr>
  {{/each}}
</tbody>