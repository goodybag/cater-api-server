<div class="matrix-editor">
  <table class="matrix-table">
    <thead>
      <tr>
        <th></th>
        {{#each set}}
          <th>
            <span class="val">{{this}}</span>
          </th>
        {{/each}}
      </tr>
    </thead>
    <tbody>
      {{#each set}}
        <tr>
          <td>{{this}}</td>
          {{#each ../set}}
            <td>
              {{ref (ref ../../values ../this) this}}
            </td>
          {{/each}}
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>