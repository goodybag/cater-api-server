            <tr>
              <td style="width: 100%; padding-top: 32px"></td>
            </tr>
            <!--</Gutter>-->

            <tr>
              <td style="width: 100%">

                {{{block "body"}}}

                <!--<NormalText>-->
                <table cellpadding="0" cellspacing="0" style="width: 100%;">
                  <tr>
                    <td style="width: 100%; padding-top: 22px; padding-bottom: 16px; padding-left: 10px; padding-right: 10px; line-height: 22px; font-weight: 300; font-family: Helvetica, arial, sans-serif; color: #695A42; font-size: 13px; text-align: center;">
                      If you have any questions, please contact us at <a style="color: #1155cc" href="mailto:orders@goodybag.com">orders@goodybag.com</a>
