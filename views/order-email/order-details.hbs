<!--<OrderDetails>-->
<table cellpadding="0" cellspacing="0" style="width: 100%">
  <tr>
    <td class="main-width-gutter" width="50%"></td>
    <td class="main-width-constraint" width="400" style="width: 400px">
      <table class="main-width-constraint" cellpadding="0" cellspacing="0" style="width: 400px; margin-left: auto; margin-right: auto; background: #ffffff; border: solid #ccccc0 1px; border-radius: 4px">
      <!--<OrderDetailsHeader>-->
      <tr>
        <td colspan="2" style="width: 100%; padding-top: 13px; padding-bottom: 10px; font-size: 24px; font-weight: 300; font-family: Helvetica, arial, sans-serif; color: #695a42; text-align: center; border-bottom: solid #CCCCC0 1px">Order Details</td>
      </tr>
      <!--</OrderDetailsHeader>-->

      <!--<OrderDetailsSpacer>-->
      <tr>
        <td colspan="2" style="width: 100%; padding-top: 16px"></td>
      </tr>
      <!--</OrderDetailsSpacer>-->

      <!--<OrderDetailsBody>-->
      {{#if this.user.organization}}
      <tr>
        <td style="font-family: Helvetica, arial, sans-serif; color: #695A42; font-size: 14px; width: 50%; padding-top: 10px; padding-bottom: 10px; padding-right: 10px; text-align: right">Customer Organization:</td>
        <td style="font-family: Helvetica, arial, sans-serif; color: #695A42; font-size: 14px; width: 50%; padding-top: 10px; padding-bottom: 10px; padding-left: 10px; text-align: right;">{{this.user.organization}}</td>
      </tr>
      {{/if}}
      <tr>
        <td style="font-family: Helvetica, arial, sans-serif; color: #695A42; font-size: 14px; width: 50%; padding-top: 10px; padding-bottom: 10px; padding-right: 10px; text-align: right">Delivery Date:</td>
        <td style="font-family: Helvetica, arial, sans-serif; color: #695A42; font-size: 14px; width: 50%; padding-top: 10px; padding-bottom: 10px; padding-left: 10px; text-align: right;">{{datepart this.datetime}}</td>
      </tr>
      <tr>
        <td style="font-family: Helvetica, arial, sans-serif; color: #695A42; font-size: 14px; width: 50%; padding-top: 10px; padding-bottom: 10px; padding-right: 10px; text-align: right">Delivery Time:</td>
        <td style="font-family: Helvetica, arial, sans-serif; color: #695A42; font-size: 14px; width: 50%; padding-top: 10px; padding-bottom: 10px; padding-left: 10px; text-align: right;">{{timepart this.datetime}}</td>
      </tr>
      <tr>
        <td style="font-family: Helvetica, arial, sans-serif; color: #695A42; font-size: 14px; width: 50%; padding-top: 10px; padding-bottom: 10px; padding-right: 10px; text-align: right">Guests:</td>
        <td style="font-family: Helvetica, arial, sans-serif; color: #695A42; font-size: 14px; width: 50%; padding-top: 10px; padding-bottom: 10px; padding-left: 10px; text-align: right;">{{this.guests}}</td>
      </tr>
      <tr>
        <td style="font-family: Helvetica, arial, sans-serif; color: #695A42; font-size: 14px; width: 50%; padding-top: 10px; padding-bottom: 10px; padding-right: 10px; text-align: right">Subtotal:</td>
        <td style="font-family: Helvetica, arial, sans-serif; color: #695A42; font-size: 14px; width: 50%; padding-top: 10px; padding-bottom: 10px; padding-left: 10px; text-align: right;">${{dollars this.sub_total}}</td>
      </tr>
      <!--</OrderDetailsBody>-->

      <!--<OrderDetailsSpacer>-->
      <tr>
        <td colspan="2" style="width: 100%; padding-top: 10px"></td>
      </tr>
      <!--</OrderDetailsSpacer>-->
    </table>
    </td>
    <td class="main-width-gutter" width="50%"></td>
  </tr>
  <tr>
    <td colspan="3" style="width: 100%; padding-top: 20px"></td>
  </tr>
</table>
<!--</OrderDetails>-->