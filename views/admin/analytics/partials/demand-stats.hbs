<section class="analytics-section analytics-section-month">
  <div class="container">
    <div class="col-4 analytics-col">
      <div>${{commatize (dollars stats.month.volume)}}</div>
      <small>Total Volume</small>
    </div>
    <div class="col-4 analytics-col">
      <div>{{commatize stats.month.placed}}</div>
      <small>Accepted Orders Placed</small>
    </div>
    <div class="col-4 analytics-col">
      <div>{{commatize stats.month.guests}}</div>
      <small>Guests Fed</small>
    </div>
  </div>
</section>
