<td class="item-name">
  {{name}}
  {{log this}}
  {{#if selectedOptions}}
  <ul class="comma-separated item-options">
    {{#each selectedOptions}}
      <li>{{this.name}}</li>
    {{/each}}
  </ul>
  {{/if}}
  <a class="item-edit" href="#" data-id="{{id}}">Edit</a>
</td>
<td class="item-quantity">x {{quantity}}</td>
<td class="item-price">{{dollars sub_total}}</td>